{
  "name": "acidjazz/metapi",
  "type": "library",
  "description": "Laravel API helpers",
  "license": "MIT",
  "keywords": [
    "Laravel", "API", "Pagination", "PHP"
  ],
  "homepage": "https://github.com/acidjazz/metapi",
  "authors": [
    {
      "name": "Kevin Olson",
      "email": "acidjazz@gmail.com"
    }
  ],
  "require": {
    "illuminate/support": "5.* || 6.* || 7.* || 8.*",
    "jasongrimes/paginator": "^1.0",
    "php": "^7.4 || ^8.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "acidjazz\\metapi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "acidjazz\\metapi\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "acidjazz\\metapi\\ServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.2",
    "phpunit/phpunit": "^9.1",
    "mockery/mockery": "^1.3"
  }
}
